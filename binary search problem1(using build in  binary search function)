#include <iostream>
#include<algorithm>
using namespace std;

int main() {
   int arr[]={ 1,2,3,4,5,6};
   int size = 6;
    if(binary_search(arr[0],arr[size-1],6)) //this is the build in binary search function;
    {
      cout<<"found the number"<<endl;
    }
    else
    {
     cout<<" not found number"<<endl;
    }
  return 0;
}
