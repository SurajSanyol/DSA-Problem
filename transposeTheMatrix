#include <iostream>
using namespace std;
int main() {
  int arr[3][3] = {{2, 4, 6}, {1, 3, 5}, {9, 11, 13}};
  int transpose[3][3];
  for (int i = 0; i < 3; i++) {
    for (int j = 0; j < 3; j++) {
      transpose[j][i]=arr[i][j];
    }
  }

  for (int i = 0; i < 3; i++) {
    for (int j = 0; j < 3; j++) {
      cout << transpose[i][j] << " ";
    }
    cout<<endl;
  }
  return 0;
}
